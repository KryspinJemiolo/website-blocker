{
    "name": "Website Blocker",
    "version": "0.1.0",
    "description": "A simple website blocker, chrome extension in javascript",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": ["main.js"]
    },
    "icons": {
        "16": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
    "manifest_version": 2
}
